        "import numpy as np\n",
        "\n",
        "arr = np.array([[1,3,9], [4,1,2]])\n",
        "\n",
        "print('Original Array:\\n',arr)\n",
        "\n",
        "arr_transpose = arr.transpose()\n",
        "\n",
        "print('Transposed Array:\\n',arr_transpose)"
      ],
      "execution_count": 2,
      "outputs": [
        {
          "output_type": "stream",
